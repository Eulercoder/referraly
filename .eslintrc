{

  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "plugins": [
    "flowtype"
  ],
  "rules":{
    "no-unused-vars" : 0,
    "object-shorthand":0,
    "no-shadow":0,
    "no-param-reassign":0,
    "max-len":0,
    "camelcase":0,
    "quote-props":0,
    "spaced-line-comment": 0,
    "spaced-comment":0,
    "no-useless-escape": 0,
    "func-names": 0,
    "array-callback-return": 0,
    "prefer-arrow-callback":0,
    "no-lonely-if":0,
    "no-underscore-dangle":0,
  }
}
